			<!-- METADATA -->
			<timeDimEnabled>true</timeDimEnabled>
            
			<!-- LIST, CONTINUOUS_INTERVAL, DISCRETE_INTERVAL -->
            <!-- timePresentationMode>CONTINUOUS_INTERVAL</timePresentationMode -->
            <timePresentationMode>DISCRETE_INTERVAL</timePresentationMode>
