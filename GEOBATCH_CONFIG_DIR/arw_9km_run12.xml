			<elevDimEnabled>true</elevDimEnabled>
			<elevationPresentationMode>LIST</elevationPresentationMode>
			<!--  <dirName>DIR</dirName>-->
			<styles/>
			
			<!--<datastorePropertiesPath>config/datastore.properties</datastorePropertiesPath>-->
